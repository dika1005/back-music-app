{
  "name": "app-music",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" --ignore-path .gitignore \"!src/generated/**\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" --ignore-path .gitignore \"!src/generated/**\"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --ignore-pattern \"src/generated/**\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix --ignore-pattern \"src/generated/**\"",
    "db:push": "bunx prisma db push",
    "db:generate": "bunx prisma generate"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/adapter-mariadb": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "dotenv": "^17.2.3",
    "elysia": "latest",
    "mariadb": "^3.4.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "bun-types": "latest",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0",
    "prismabox": "^1.1.26"
  },
  "module": "src/index.js"
}
